+ </repeat>
+ <wait>1</wait>
+ <fire>
+  <direction type="sequence">0</direction>
+  <speed type="sequence">-0.1</speed>
+  <bulletRef label="3way"/>
+ </fire>
+ <repeat> <times>2</times>
+ <action>
+  <fire>
+   <direction type="sequence">20</direction>
+   <speed type="sequence">0</speed>
+   <bulletRef label="3way"/>
+  </fire>
+ </action>
+ </repeat>
+ <wait>1</wait>
+</action>
+</repeat>
+</action>
+</bullet>
+
+<bullet label="3way">
+<speed>1.8</speed>
+</bullet>
+
+</bulletml>
--- bulletml-0.0.6.orig/debian/doc/sample/[Progear]_round_6_boss_parabola_shot.xml
+++ bulletml-0.0.6/debian/doc/sample/[Progear]_round_6_boss_parabola_shot.xml
