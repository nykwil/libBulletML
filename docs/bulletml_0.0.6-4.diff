--- bulletml-0.0.6.orig/debian/doc/sample/[Progear]_round_5_middle_boss_rockets.xml
+++ bulletml-0.0.6/debian/doc/sample/[Progear]_round_5_middle_boss_rockets.xml
@@ -0,0 +1,41 @@
+<?xml version="1.0" ?>
